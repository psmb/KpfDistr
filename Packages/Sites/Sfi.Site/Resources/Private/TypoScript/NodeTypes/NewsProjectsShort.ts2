prototype(Sfi.Site:NewsProjectsShort) < prototype(TYPO3.TypoScript:Template) {
	node = ${node}
	templatePath = 'resource://Sfi.Site/Private/Templates/NodeTypes/NewsProjectsShort.html'
	title = ${String.stripTags(node.properties.title)}
	teaser = ${String.stripTags(node.properties.teaser)}
	image = TYPO3.Neos:ImageUri {
		asset = ${q(node).property('imageNormal')}
		width = 370
		height = 243
		allowCropping = TRUE
	}
}
