prototype(Sfi.Site:NewsBlogShort) < prototype(TYPO3.TypoScript:Template) {
	node = ${node}
	templatePath = 'resource://Sfi.Site/Private/Templates/NodeTypes/NewsBlogShort.html'
	title = ${String.stripTags(node.properties.title)}
	teaser = ${String.stripTags(node.properties.teaser)}
	image = TYPO3.Neos:ImageUri {
		asset = ${q(node).find('[instanceof TYPO3.Neos.NodeTypes:Image]').property('image')}
		width = 699
		height = 367
		allowCropping = TRUE
	}
}
