prototype(Sfi.Site:NewsTopShort) < prototype(TYPO3.TypoScript:Template) {
	node = ${node}
	templatePath = 'resource://Sfi.Site/Private/Templates/NodeTypes/NewsTopShort.html'
	title = ${String.stripTags(node.properties.title)}
	teaser = ${String.stripTags(node.properties.teaser)}
	image = TYPO3.Neos:ImageUri {
		asset = ${node.properties.topCoverImage && node.properties.coverImage}
		width = 1168
		height = 437
		allowCropping = TRUE
		allowUpScaling = TRUE
	}
}
